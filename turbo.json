{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"link": {
			"dependsOn": ["build"]
		},
		"clean": {
			"cache": false
		},
		"test:ci": {
			"cache": false
		},
		"test": {
			"cache": false
		},
		"test:win": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", "dist/**"]
		},
		"create": {
			"dependsOn": ["^create"],
			"cache": false
		}
	}
}
