{
	"name": "@codemod-com/backend",
	"version": "0.0.78",
	"scripts": {
		"build": "node esbuild.config.js",
		"start": "prisma -v && pnpm run db:migrate:apply && node build/index.js",
		"start:local": "node build/index.js",
		"test": "docker-compose up -d db-test --wait && dotenv -e .env.test -- pnpm prisma migrate deploy && dotenv -e .env.test -- vitest --run && docker-compose down db-test",
		"watch": "pnpm db:generate && nodemon --exec ./node_modules/.bin/ts-node --esm ./src/index.ts",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:migrate:generate": "prisma migrate dev --name",
		"db:migrate:apply": "prisma migrate deploy",
		"db:seed": "tsx prisma/seed.ts",
		"db:studio": "prisma studio"
	},
	"author": "Codemod inc.",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@codemod-com/tsconfig": "workspace:*",
