{
	"name": "@codemod-com/backend",
	"version": "0.0.83",
	"scripts": {
		"build": "node esbuild.config.js",
		"start": "prisma -v && pnpm run db:migrate:apply && node build/index.js",
		"start:local": "node build/index.js",
		"test": "docker-compose up -d db-test --wait && dotenv -e .env.test -- pnpm prisma migrate deploy && dotenv -e .env.test -- vitest --run && docker-compose down db-test",
		"watch": "pnpm db:generate && nodemon --exec ./node_modules/.bin/ts-node --esm ./src/index.ts",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:migrate:generate": "prisma migrate dev --name",
		"db:migrate:apply": "prisma migrate deploy",
		"db:seed": "tsx prisma/seed.ts",
		"db:studio": "prisma studio"
	},
	"author": "Codemod inc.",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@codemod-com/tsconfig": "workspace:*",
		"@faker-js/faker": "^8.4.1",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "20.10.5",
		"@types/parse-github-url": "1.0.3",
		"@types/pg": "^8.11.2",
		"@types/semver": "7.5.8",
		"@types/supertest": "^6.0.2",
		"dotenv-cli": "^7.4.1",
		"esbuild": "^0.19.12",
		"fastify-plugin": "4.5.1",
		"nodemon": "3.0.2",
		"sqlite3": "^5.1.7",
		"supertest": "^6.3.4",
		"ts-node": "10.9.2",
		"tsx": "^4.7.1",
		"typescript": "5.3.3",
		"vitest": "1.1.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.549.0",
		"@aws-sdk/s3-request-presigner": "^3.549.0",
		"@clerk/backend": "0.36.0",
		"@clerk/fastify": "0.6.27",
		"@codemod-com/utilities": "workspace:*",
		"@fastify/busboy": "^2.1.1",
		"@fastify/cors": "8.5.0",
		"@fastify/multipart": "^8.1.0",
		"@fastify/rate-limit": "9.0.1",
		"@prisma/client": "5.10.2",
		"@types/tar": "^6.1.11",
		"ai": "2.2.29",
		"axios": "^1.6.8",
		"chatgpt": "5.2.5",
		"chromadb": "1.7.2",
		"cron": "^3.1.6",
		"dotenv": "^16.4.5",
		"fastify": "4.25.1",
		"fuse.js": "^7.0.0",
		"langchain": "0.0.209",
		"lru-cache": "10.1.0",
		"openai": "4.23.0",
		"openai-edge": "1.2.2",
		"parse-github-url": "1.0.2",
		"pg": "^8.11.3",
		"prisma": "^5.10.2",
		"prisma-json-types-generator": "^3.0.4",
		"replicate": "0.25.2",
		"semver": "7.6.0",
		"sequelize": "^6.37.1",
		"tar": "^6.2.0",
		"valibot": "^0.24.1",
