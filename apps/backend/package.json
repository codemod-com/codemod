		"typescript": "5.3.3",
		"vitest": "1.1.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "catalog:",
		"@aws-sdk/s3-request-presigner": "catalog:",
		"@clerk/backend": "catalog:",
		"@clerk/fastify": "catalog:",
		"@codemod-com/utilities": "workspace:*",
		"@fastify/busboy": "catalog:",
