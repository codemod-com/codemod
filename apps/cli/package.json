{
  "name": "codemod",
  "author": "Codemod, Inc.",
  "imports": {
    "#*": "./src/*"
  },
  "version": "0.18.9",
  "description": "A codemod engine for Node.js libraries (jscodeshift, ts-morph, etc.)",
  "type": "module",
  "exports": null,
  "repository": {
    "type": "git",
    "url": "https://github.com/codemod-com/codemod.git",
    "directory": "apps/cli"
  },
  "keywords": [
    "codemod",
    "jscodeshift",
    "ts-morph",
    "refactor",
    "transform",
    "cli",
    "dependencies",
    "migrate",
    "eslint",
    "prettier",
    "progress",
    "AST",
    "next.js",
    "ember",
    "react"
  ],
  "main": "./dist/index.cjs",
  "bin": "./dist/index.cjs",
  "devDependencies": {
    "@codemod-com/api-types": "workspace:*",
    "@codemod-com/filemod": "workspace:*",
    "@codemod-com/printer": "workspace:*",
    "@codemod-com/runner": "workspace:*",
    "@codemod-com/telemetry": "workspace:*",
    "@codemod-com/utilities": "workspace:*",
    "@types/blessed": "^0.1.25",
    "@types/columnify": "^1.5.4",
    "@types/diff": "catalog:",
    "@types/inquirer": "catalog:",
    "@types/node": "catalog:",
    "@types/prettyjson": "^0.0.33",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.33",
    "axios": "catalog:",
    "columnify": "1.6.0",
    "diff": "catalog:",
    "exponential-backoff": "^3.1.2",
    "glob": "catalog:",
    "inquirer": "catalog:",
    "memfs": "^4.6.0",
    "open": "^10.1.2",
    "prettyjson": "^1.2.5",
    "semver": "^7.6.2",
    "terminal-link": "^4.0.0",
    "ts-morph": "18.0.0",
    "valibot": "catalog:",
    "vitest": "^1.0.1",
    "yargs": "^18.0.0"
  },
  "scripts": {
    "build": "tsc && node ./build.js",
    "build:local": "pnpm build -- --local",
    "build:prod": "pnpm build -- --prod",
    "build:publish": "pnpm build -- --publish",
    "package": "pkg --compress GZip .",
    "test": "TEST=1 vitest run",
    "coverage": "TEST=1 vitest run --coverage",
    "prepublishOnly": "turbo run build:publish --filter codemod"
  },
  "engines": {
    "node": ">=18.5.0"
  },
  "pkg": {
    "outputPath": "./package/"
  },
  "license": "Apache License, Version 2.0",
  "files": ["./dist/index.cjs", "LICENSE", "README.md"],
  "publishConfig": {
    "access": "public",
    "tag": "legacy"
  },
  "dependencies": {
    "@ast-grep/cli": "catalog:",
    "@ast-grep/napi": "catalog:",
    "@codemod.com/workflow": "workspace:*",
    "@octokit/rest": "catalog:",
    "blessed": "^0.1.81",
    "esbuild": "^0.23.0",
    "keytar": "catalog:"
  }
}
