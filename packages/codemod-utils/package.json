{
  "name": "@codemod.com/codemod-utils",
  "author": "Codemod, Inc.",
  "imports": {
    "#*": "./src/*"
  },
  "version": "1.1.3",
  "description": "Set of utilities for jscodeshift",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.cts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "./dist",
    "!**/*.test.js",
    "!**/*.test.d.ts",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "turbo run build --filter @codemod.com/codemod-utils"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jscodeshift": "^0.11.11",
    "@types/node": "catalog:",
    "tsup": "^8.3.6",
    "typescript": "catalog:",
    "vitest": "^1.0.1"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/parser": "^7.24.4",
    "jscodeshift": "^0.16.1"
  }
}
