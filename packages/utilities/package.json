{
	"name": "@codemod-com/utilities",
	"author": "Codemod, Inc.",
	"version": "1.1.0",
	"description": "The utilities used across Codemod.com packages",
	"type": "module",
	"main": "./dist/index.js",
	"exports": {
		".": "./src/index.ts",
		"./functions/validationMethods": "./src/functions/validationMethods.ts"
	},
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"!**/*.test.js",
		"!**/*.test.d.ts",
		"LICENSE",
		"README.md",
		"tsconfig.json"
	],
	"scripts": {
		"build": "tsc",
		"lint:eslint:check": "npx @biomejs/biome lint . --no-color",
		"lint:prettier:check": "npx @biomejs/biome format . --check --no-color ",
		"lint:eslint:write": "npx @biomejs/biome lint . --apply",
		"lint:prettier:write": "npx @biomejs/biome format --write .",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"keywords": [],
	"license": "Apache-2.0",
	"devDependencies": {
		"@types/jscodeshift": "^0.11.11",
		"@types/node": "20.10.3",
		"@vitest/coverage-v8": "^1.0.1",
		"memfs": "^4.6.0",
		"ts-node": "10.9.1",
		"typescript": "^5.3.2",
		"vitest": "^1.0.1",
		"@biomejs/biome": "1.5.3"
	},
	"dependencies": {
		"@codemod-com/filemod": "workspace:*",
		"@effect/schema": "^0.52.0",
		"fast-glob": "3.3.2",
		"jscodeshift": "^0.15.1"
	}
}
