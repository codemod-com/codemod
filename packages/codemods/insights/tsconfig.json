{
  "extends": "@codemod-com/tsconfig/codemod.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.js",
    "./test/**/*.ts",
    "./test/**/*.js"
  ],
  "exclude": ["node_modules", "./dist/**/*"],
  "ts-node": {
    "transpileOnly": true
  }
}
